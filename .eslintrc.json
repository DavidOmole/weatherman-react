{
    "extends": "react-app",
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
            "jsx": true
      } 
    },
    "rules": {
      "semi": "error",
      "no-console": "warn",
      "no-var": "error",
      "prefer-const" : "warn",
      "template-curly-spacing": "warn"
    }
}